                                    "number": "10",
                                    "predecessors": [
                                        
                                    ],
                                    "heading": "Validation Environment",
                                    "documents": [
                                        "CFTP"
                                    ],
                                    "versions": [
                                        "1.0"
                                    ],
                                    "section": "sec10",
                                    "content": "<p><font color=\"blue\">Description of the Environment of the system that will be used for test execution.</font></p>",
                                    "key": "FRML24113-101",
                                    "status": "Done"
                                },
